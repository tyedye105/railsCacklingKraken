<%= render "layouts/errors", :object => @product %>
<div class="well">
<%= form_for @product do |f| %>
<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div class="form-group">
  <%= f.label :description %>
  <%= f.text_field :description %>
</div>
<div class="form-group">
  <%= f.label :found_in %>
  <%= f.text_field :found_in %>
</div>
<div class="form-group">
  <%= f.label :going_price %>
  <%= f.number_field :going_price %>
</div>
<%= f.submit %>
<% end %>
</div>
